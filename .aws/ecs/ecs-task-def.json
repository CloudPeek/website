{
  "family": "cloudpeek-website",
  "containerDefinitions": [
      {
          "name": "cloudpeek",
          "image": "cloudpeek/website:latest",
          "cpu": "256",
          "memory": 512,
          "portMappings": [
              {
                  "name": "cloudpeek-3000-tcp",
                  "containerPort": 3000,
                  "hostPort": 3000,
                  "protocol": "tcp"
              }
          ],
          "essential": true,
          "environment": [],
          "mountPoints": [],
          "volumesFrom": [],
          "systemControls": []
      }
  ],
  "executionRoleArn": "arn:aws:iam::661857570768:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
      "EC2"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  }
}